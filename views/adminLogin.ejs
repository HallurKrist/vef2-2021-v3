<%- include('header') -%>


<dev class='pagecontainer'>
  <h1 class='title'>Innskráning</h1>
  <form method="post" action="/admin/login" class="loginForm" enctype="application/x-www-form-urlencoded">
    <dev class="form__input">
      <label for="username" class="form__label">
        Notendanafn
      </label>
      <input
      required
      id="username"
      type="text"
      name="username"
      value=""
      class="form__text"
      >
    </dev>
    <dev class="form__input">
      <label for="password" class="form__label">
        Lykilorð
      </label>
      <input
      id="password"
      type="password"
      name="password"
      value=""
      class="form__text"
      >
    </dev>
    <dev class="form__login__error">
      <p><%- loginErrors %></p>
    </dev>
    <div class="form__login__button">
      <button class="form__button__login">Innskrá</button>
    </div>
  </form>
  <a href = "../">Til baka</a>

</dev>

<%- include('footer') -%>
